{
  "name": "token-customer-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "node patch.js",
    "ng": "ng",
    "serve-en": "ng serve --configuration=en --host 0.0.0.0 --disable-host-check",
    "serve-de": "ng serve --configuration=de --port=4201",
    "build": "npm run build-all",
    "build-all": "run-p \"build:*\"",
    "build:en": "ng build --configuration=en-prod",
    "build:de": "ng build --configuration=de-prod",
    "i18n": "npm run i18n-extract && npm run i18n-clear",
    "i18n-extract": "ng xi18n --out-file messages.generated.xlf --output-path locale",
    "i18n-clear": "node i18n-clear.js",
    "test": "ng test",
    "hooks:pre-commit": "npm-run-all prettier:fix-staged",
    "hooks:pre-push": "npm-run-all lint",
    "prettier:fix-staged": "pretty-quick --staged",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all hooks:pre-commit"
    }
  },
  "dependencies": {
    "@angular/animations": "^7.2.16",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.16",
    "@angular/compiler": "^7.2.16",
    "@angular/core": "^7.2.16",
    "@angular/forms": "^7.2.16",
    "@angular/material": "^7.3.7",
    "@angular/material-moment-adapter": "^7.3.7",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "^7.2.16",
    "@angular/router": "^7.2.16",
    "@types/web3": "1.0.19",
    "core-js": "2.5.4",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "ngx-api-utils": "1.0.5",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "web3": "1.2.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.7",
    "@angular/compiler-cli": "^7.2.16",
    "@angular/language-service": "^7.2.16",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^10.17.16",
    "codelyzer": "~4.5.0",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.10.0",
    "protractor": "^5.4.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "~3.2.2"
  }
}
